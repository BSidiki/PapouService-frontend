{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/name-role-value": [
      "off",
      {
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "hyphens",
          "backdrop-filter",
          "-webkit-overflow-scrolling",
          "user-select",
          "user-select"
        ]
      }
    ],
    "disown-opener": "off",
    "button-type": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 18.3",
    "not safari <= 18.3"
  ]
}